<h1>Listing audio_stories</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Title</th>
      <th>Url</th>
      <th>Source</th>
      <th>Program</th>
      <th>Duration</th>
      <th>Audio url</th>
      <th>Time stamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @audio_stories.each do |audio_story| %>
      <tr>
        <td><%= audio_story.category %></td>
        <td><%= audio_story.title %></td>
        <td><%= audio_story.url %></td>
        <td><%= audio_story.source %></td>
        <td><%= audio_story.program %></td>
        <td><%= audio_story.duration %></td>
        <td><%= audio_story.audio_url %></td>
        <td><%= audio_story.time_stamp %></td>
        <td><%= link_to 'Show', audio_story %></td>
        <td><%= link_to 'Edit', edit_audio_story_path(audio_story) %></td>
        <td><%= link_to 'Destroy', audio_story, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audio story', new_audio_story_path %>
